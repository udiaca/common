  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Jest Test",
      "type": "node",
      "request": "launch",
      "preLaunchTask": "npm: build",
      "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
      "runtimeArgs": [
        "-i"
      ],
      "args": [
        "--testTimeout",
        "3600000",
        "-t",
        "should capture delete of nested deep property child"
      ],
      "console": "integratedTerminal"
    }
  ]
}